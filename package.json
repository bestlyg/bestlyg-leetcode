{
  "name": "bestlyg-leetcode",
  "version": "1.0.0",
  "description": "个人LeetCode题解",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write **/*.md",
    "gen:env": "node utils/gen/genEnv.js",
    "gen:solve": "node utils/gen/genSolve.js",
    "gen:newSolve": "node utils/gen/genNewSolve.js",
    "gen:README.md": "node utils/gen/genReadme.js",
    "run:node": "node temp/index.js",
    "run:ts-node": "ts-node temp/index.ts",
    "commit": "git-cz",
    "push:auto": "git pull && npm run gen:README.md && npm run format && git add -A && git-cz && git push origin master"
  },
  "repository": {
    "type": "git",
    "url": "https://gitee.com/bestlyg/bestlyg-leetcode.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "commitizen": "^4.1.1",
    "cz-conventional-changelog": "^3.2.0",
    "execa": "^4.0.2",
    "fs-extra": "^9.0.1",
    "git-cz": "^4.3.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.3"
  }
}
